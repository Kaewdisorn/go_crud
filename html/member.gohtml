<!DOCTYPE html>
<html>
<head>
<meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body>

    <h1>WELCOME</h1>
    <table border="1">
        <thead>
            <tr>
                <td>ID</td>
                <td>Username</td>
                <td>Email</td>
            </tr>
        </thead>
        <tbody>
    {{ if . }}
        {{ range . }}
        <tr>
            <td>{{ .ID }}</td>
            <td> {{ .Username }} </td>
            <td>{{ .Email }} </td> 
            <td>
            <a onclick="alert('DONE!')" href="/deleteUsers?id={{.ID}}">Delete</a> |
            <a href="/editUsers?id={{.ID}}">Edit</a>
          </td>
        </tr>
        {{ end }}
    {{ end }}
       </tbody>
    </table>
</body>
</html>